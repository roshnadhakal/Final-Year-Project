{
  "extends": "expo/tsconfig.base",
  "compilerOptions": {
  "target": "ES2021",
  "module": "ESNext",
  "moduleResolution": "bundler",
  "jsx": "react-native",
  "strict": true,
  "noEmit": true,
  "isolatedModules": true,
  "esModuleInterop": true,
  "resolveJsonModule": true,
  "allowJs": true,
  "types": ["react", "react-native"],
  "baseUrl": ".",
  "paths": {
    "@/*": ["./*"],
    "@css/*": ["app/css/*"],
    "@config": ["src/config/config"],
    "@context/*": ["context/AuthContext"],
    "@websocketcontext/*":["Chat/WebSocketContext"]
  }
},
  "include": [
    "**/*.ts",
    "**/*.tsx",
    "**/*.d.ts",
    ".expo/types/**/*.ts",
    "expo-env.d.ts"
  ]
}
